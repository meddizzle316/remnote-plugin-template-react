        ],
        "text": [
            {
                "text": "Adding to your Path ",
                "i": "m",
                "b": true
            }
        ]
    },
    {
        "_id": "EfrTg52sd1UkKWB9G",
        "createdAt": 1731961695797,
        "updatedAt": 1731961703652,
        "parent": "fSd0YS1Hz5ZGgvF7m",
        "children": [
            "47OOo1PwsmvP7V0RD"
        ],
        "text": [
            "to the beginning of the existing PATH variable"
        ]
    },
    {
        "_id": "47OOo1PwsmvP7V0RD",
        "createdAt": 1731961695797,
        "updatedAt": 1731961703652,
        "parent": "EfrTg52sd1UkKWB9G",
        "children": [
